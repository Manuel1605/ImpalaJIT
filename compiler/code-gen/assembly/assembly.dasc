void Assembly::pushLocalVariableToFPUStack(int index) {
// printf("Val: %u\n", index);
    | fld qword [rbp-(16+index*8)];
}

void Assembly::replaceLocalVariable(int index) {
// printf("Val: %u\n", index);
    | fstp qword [rbp-(16+index*8)];
}

